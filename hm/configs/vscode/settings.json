{
    "[crystal]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": false
        },
        "editor.formatOnPaste": false,
        "editor.formatOnType": false
    },
    "autoDocstring.quoteStyle": "'''",
    "autoDocstring.startOnNewLine": true,
    "bookmarks.gutterIconBorderColor": "#FFFFFF",
    "bookmarks.keepBookmarksOnLineDelete": true,
    "breadcrumbs.enabled": true,
    "conventionalCommits.autoCommit": false,
    "conventionalCommits.gitmoji": false,
    "conventionalCommits.showEditor": true,
    "conventionalCommits.showNewVersionNotes": false,
    "coverage-gutters.coverageFileNames": [
        "lcov.info",
        "cov.xml",
        "coverage.xml",
        "jacoco.xml"
    ],
    "coverage-gutters.coverageReportFileName": "coverage/index.html",
    "coverage-gutters.ignoredPathGlobs": "**/{node_modules,venv,.venv,vendor,lib,bin,.direnv}/**",
    "css.lint.float": "warning",
    "dart.debugExternalPackageLibraries": false,
    "dart.debugSdkLibraries": false,
    "debug.inlineValues": "auto",
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.maxComputationTime": 3000,
    "diffEditor.renderSideBySide": true,
    "docwriter.progress.trackFunctions": false,
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "line-thin",
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.loop": false,
    "editor.fontFamily": "'PT Mono', 'Jetbrains Mono', Monaco, monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 2,
    "editor.lineNumbers": "relative",
    "editor.links": true,
    "editor.matchBrackets": "always",
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.scale": 2,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "trailing",
    "editor.rulers": [
        80,
        96
    ],
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "bottom",
    "editor.stickyScroll.enabled": true,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "errorLens.fontStyleItalic": true,
    "errorLens.gutterIconsEnabled": true,
    "explorer.compactFolders": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "*.vala": "java"
    },
    "files.autoSave": "onFocusChange",
    "git.autoStash": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.ignoreMissingGitWarning": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.statusBar.enabled": false,
    "gremlins.gutterIconSize": "100%",
    "gremlins.showInProblemPane": true,
    "http.systemCertificates": false,
    "lldb.evaluationTimeout": 1000,
    "lldb.launch.expressions": "native",
    "lldb.showDisassembly": "never",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.saturation": 0.9,
    "nix.serverPath": "rnix-lsp",
    "npm.enableRunFromFolder": true,
    "npm.exclude": "**/{node_modules,,lib}/**",
    "python.formatting.provider": "black",
    "python.languageServer": "Pylance",
    "python.linting.banditEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.lintOnSave": false,
    "python.linting.pylintEnabled": false,
    "python.terminal.activateEnvironment": false,
    "python.terminal.executeInFileDir": true,
    "redhat.telemetry.enabled": true,
    "scm.defaultViewMode": "tree",
    "security.workspace.trust.banner": "never",
    "svg.preview.autoOpen": true,
    "task.autoDetect": "off",
    "taskExplorer.enableExplorerView": false,
    "taskExplorer.enableSideBar": true,
    "taskExplorer.numLastTasks": 5,
    "taskExplorer.showFavoritesButton": false,
    "taskExplorer.showLastTasks": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.tabs.enabled": true,
    "terminal.integrated.shell.osx": "fish",
    "todo-tree.filtering.excludeGlobs": [
        "**/node_modules",
        "**/.direnv"
    ],
    "todo-tree.filtering.ignoreGitSubmodules": true,
    "todo-tree.filtering.includeHiddenFiles": true,
    "todo-tree.filtering.useBuiltInExcludes": "file excludes",
    "todo-tree.general.statusBar": "top three",
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "DOCS",
        "NOTE",
        "FIX"
    ],
    "todo-tree.highlights.enabled": false,
    "todo-tree.highlights.useColourScheme": true,
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "todo-tree.ripgrep.ripgrep": "/home/dinko/.nix-profile/bin/rg",
    "todo-tree.tree.disableCompactFolders": true,
    "todo-tree.tree.expanded": true,
    "todo-tree.tree.flat": true,
    "todo-tree.tree.showCountsInTree": true,
    "todo-tree.tree.showCurrentScanMode": false,
    "vim.cursorStylePerMode.insert": "line-thin",
    "vim.sneak": true,
    "vsintellicode.java.completionsEnabled": false,
    "window.titleBarStyle": "custom",
    "workbench.colorCustomizations": {
        "[GitHub Dark]": {
            "editor.lineHighlightBackground": "#e2bd171c"
        },
        "[Gruvbox Dark Medium]": {
            "sideBar.background": "#2a3a48",
            "sideBar.border": "#179766"
        }
    },
    "workbench.colorTheme": "GitHub Dark",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tree.indent": 16,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.view.alwaysShowHeaderActions": true
}